fn Update() : i32 {

	Color clear_color;

	clear_color.r = 0-128;
	clear_color.g = 0-128;
	clear_color.b = 0-128;
	clear_color.a = 0-128;

	BeginDrawing();
	ClearBackground(clear_color);

	DrawFPS(60,60);

	EndDrawing();

	return 0;
}

fn main() : i32 {

	i32 flags = Window_Flags.FLAG_WINDOW_RESIZABLE;

	SetConfigFlags(flags);

	InitWindow(500, 700, "Hello Window");

	while  WindowShouldClose() != 1 {
		Update();
	}

	return 0;
}