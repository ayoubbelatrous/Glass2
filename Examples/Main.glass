fn strlen(u8* string) : i32 {

	u8 c = 1;
	u8 i = 0;

	while c {
		c = string[i];
		i = i + 1;
	}

	return i;
}

fn main() : i32 {

	printf("Hello World Langth is: %i", strlen("Hello World"));

	return 0;
}